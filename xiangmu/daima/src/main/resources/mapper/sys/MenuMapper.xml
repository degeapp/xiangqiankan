  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jianz.degeapp.dao.MenuDao">
	<select id="findMenuByUserName" resultType="map">
	select * from menu where qufen=(select qufen from user where username=#{username})

</select>

